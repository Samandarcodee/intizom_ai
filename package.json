    "react-circular-progressbar": "^2.2.0",
    "react-dom": "^19.2.0",
    "tsx": "^4.20.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "prisma": "^6.19.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
